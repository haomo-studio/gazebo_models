        </limit>
        <dynamics>
          <damping>1.0</damping>
          <friction>1.0</friction>
        </dynamics>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
      <physics>
        <ode>
          <cfm_damping>1</cfm_damping>
        </ode>
      </physics>
    </joint>

    <!-- Joint to make the button pushable -->
    <joint name="button_mechanism" type="prismatic">
